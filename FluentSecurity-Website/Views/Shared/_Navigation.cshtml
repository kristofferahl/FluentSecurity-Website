@model string
@{
	var id = Model;
	string wrapperElement = null;
	string containerElement = "p";
	if (id == "menu") {
		wrapperElement = "li";
		containerElement = "ul";
	}
}
@Html.Navigation(() => new List<MvcHtmlString> {
	@Html.NavigationLink(Url.Action<ContentController>(x => x.Index()), "Home", wrapperElement),
	@Html.NavigationLink(Url.Action<ContentController>(x => x.GettingStarted()), "Getting started", wrapperElement),
	@Html.NavigationLink(Url.Action<WikiController>(x => x.Doc("")), "Documentation", wrapperElement),
	@Html.NavigationLink(Url.Action<WikiController>(x => x.Doc(""), new { docId = "Downloads" }), "Downloads", wrapperElement, markSelectedWhenActive: false),
	@Html.NavigationLink(Url.Action<WikiController>(x => x.Doc(""), new { docId = "Contributing" }), "Contribute", wrapperElement, markSelectedWhenActive: false),
	@Html.NavigationLink(Url.Action<ContentController>(x => x.Support()), "Support", wrapperElement),
	@Html.NavigationLink(Url.Action<ContentController>(x => x.Contact()), "Contact", wrapperElement)
}, new { id }, containerElement)